<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<AnchorPane maxHeight="100.0" maxWidth="1450.0" minHeight="100.0" minWidth="1450.0" prefHeight="100.0" prefWidth="1450.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" maxHeight="100.0" maxWidth="1450.0" minHeight="100.0" minWidth="1450.0" prefHeight="100.0" prefWidth="1450.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar maxHeight="100.0" maxWidth="1482.0" minHeight="100.0" minWidth="1450.0" prefHeight="100.0" prefWidth="1482.0">
               <items>
                  <ImageView fx:id="iMatLabel" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onIMatPressed" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../../resources/Logga.PNG" />
                     </image>
                  </ImageView>
                  <Region maxHeight="100.0" maxWidth="70.0" prefHeight="100.0" prefWidth="83.0" />
                  <Button mnemonicParsing="false" styleClass="btn-nostyle">
                     <graphic>
                        <ImageView fitHeight="150.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../../resources/search.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <TextField fx:id="searchBar" minWidth="600.0" onAction="#onSearchTyped" onKeyPressed="#onSearchTyped" prefHeight="30.0" prefWidth="729.0" promptText="Sök efter produkter här">
                     <cursor>
                        <Cursor fx:constant="TEXT" />
                     </cursor>
                  </TextField>
                  <Region maxHeight="100.0" maxWidth="97.0" prefHeight="100.0" prefWidth="14.0" />
                  <HBox alignment="CENTER" maxHeight="100.0" maxWidth="314.0" prefHeight="100.0" prefWidth="314.0">
                     <children>
                        <Button fx:id="kvitton" mnemonicParsing="false" prefHeight="50.0" prefWidth="87.0" text="Kvitton">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="hjalp" mnemonicParsing="false" prefHeight="50.0" prefWidth="71.0" text="Hjälp">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                        <Button fx:id="minaSidor" mnemonicParsing="false" prefHeight="50.0" prefWidth="114.0" text="Mina Sidor">
                           <cursor>
                              <Cursor fx:constant="HAND" />
                           </cursor>
                        </Button>
                     </children>
                  </HBox>
               </items>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ToolBar>
         </children>
      </HBox>
   </children>
   <stylesheets>
      <URL value="@Navbar.css" />
      <URL value="@../../../styles.css" />
   </stylesheets>
</AnchorPane>
