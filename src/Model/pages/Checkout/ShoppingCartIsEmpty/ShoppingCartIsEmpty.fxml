<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<fx:root fx:id="rootPane" maxHeight="175.0" prefHeight="175.0" prefWidth="600.0" style="-fx-background-color: transparent;" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@../../../../styles.css" />
    </stylesheets>
   <children>
      <HBox alignment="CENTER" maxWidth="600.0" prefHeight="175.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="175.0" prefWidth="396.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="75.0" prefWidth="274.0" text="Inga varor är tillagda i varukorgen. Vill du fortsätta handla? " textAlignment="CENTER" wrapText="true">
                     <styleClass>
                        <String fx:value="text" />
                        <String fx:value="text-md" />
                     </styleClass>
                  </Label>
                  <Button fx:id="continueShoppingButton" mnemonicParsing="false" styleClass="btn-primary" text="Tryck Här" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</fx:root>
